   -
     internal_properties : CAAAAAgAAADWWsLyrACOJihxd0BoM2Io7IIs28jx9K4g3vG5C+SQMoJFRaDy/y1FvO8tuWmWENOBxs4CQ+evZD0WYHmCB0AhOyvckP6NFcB2mdQSCl909qkpiRMKpuH/5vogFKfZluV/Xl0KlnB7MdCN5H+SyueYc/yqhtVC273Hjeu6tfEqeg5J1eRKJq9evay14ZXgvPcCp/aBzewantYNioxvO7w55oi2olcPhsjkLA==
     code : |1+
      
      HCréationSiInexistant("*")
      
      gnIDBlog est un entier
      gnIDMembre est un entier
      gsSession est une chaine
      
      // Blog en cours
      gnIDBlog = val(PageParamètre("blog"))
      
      // Login du membre
      local
      	si PageParamètre("do") = "bye" alors
      		// Supprime la session en cours
      		CookieEcrit("session", "")
      		gsSession = ""
      		gnIDMembre = 0
      	sinon
      		
      		sSession est une chaine
      		gsSession = cookielit("session")
      		sSession = gsSession
      		
      		si sSession = "" alors
      			// L'utilisateur n'est pas enregistré
      			gnIDMembre = 0
      		sinon
      			sLogin est une chaine
      			sLogin = extraitchaine(sSession, 2, "-")
      			
      			HLitRecherchePremier(Membre,Pseudo,slogin)
      			si htrouve(Membre) ALORS
      				
      				sSession = Décrypte(extraitchaine(sSession, 1, "-"), "awp",crypteAucun,vrai)
      				si val(extraitchaine(sSession, 2)) = membre.idmembre alors
      					
      					// Extrait la date et heure du début de session
      					dDateHeureDébutSession est une dateheure = ExtraitChaîne(sSession, 1)
      					dDateHeureEnCours est une dateheure = datedujour + heuresys
      					
      					// Si la session a plus d'une journée
      					si val(gauche(DateHeureDifférence(dDateHeureDébutSession, dDateHeureEnCours),8)) > 0 ALORS
      						// Sauvegarde
      						CookieEcrit("session", "")				
      						// Cela fait plus d'un jour, la session a expiré
      						gnIDMembre = 0
      						// Il faut se logger
      						ScriptAffiche("login.awp?reason=expire&next=" + urldecode(PageParamètre("next")))
      					sinon
      						// On actualise la session
      						GenèreSession()					
      						gnIDMembre = membre.IDMembre
      						membre.session = gsSession
      						hmodifie(Membre)
      						
      						// Sauvegarde
      						CookieEcrit("session", gsSession, 1)
      					FIN
      				sinon
      					// Sauvegarde
      					CookieEcrit("session", "")				
      					gnIDMembre = 0	
      					ScriptAffiche("login.awp?reason=invalid&next=" + urldecode(PageParamètre("next")))			
      				fin
      			sinon
      				// Sauvegarde
      				CookieEcrit("session", "")				
      				gnIDMembre = 0	
      				ScriptAffiche("login.awp?reason=notfound&next=" + urldecode(PageParamètre("next")))
      			FIN
      		FIN		
      		
      	FIN	
      	
      
